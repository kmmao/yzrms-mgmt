<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>HttpRPCServlet</servlet-name>
        <servlet-class>com.nazca.io.httprpc.server.DefaultHttpRPCServlet</servlet-class>
        <init-param>
            <param-name>projectName</param-name>
            <param-value>yzrms-server</param-value>
        </init-param>
    </servlet>
<!--    <servlet>
        <servlet-name>InitServelt</servlet-name>
        <servlet-class>com.yz.rms.server.util.InitServelt</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>-->
    <servlet-mapping>
        <servlet-name>HttpRPCServlet</servlet-name>
        <url-pattern>/rpc</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
